    "import pyttsx3\n",
    "\n",
    "engine = pyttsx3.init()\n",
    "\n",
    "voices = engine.getProperty(\"voices\")\n",
    "engine.setProperty(\"voice\", voices[26].id)\n",
    "\n",
    "def Speak(Audio):\n",
    "    engine.say(Audio)\n",
    "    engine.runAndWait()\n",
    "    \n",
    "text = input(\"Enter your text: \")\n",
    "\n",
    "Speak(text)"
   ]
